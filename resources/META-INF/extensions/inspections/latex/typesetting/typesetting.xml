<idea-plugin>
    <extensions defaultExtensionNs="com.intellij">
        <localInspection language="Latex" implementationClass="nl.hannahsten.texifyidea.inspections.latex.typesetting.LatexIncorrectSectionNestingInspection"
                         groupPath="LaTeX" groupName="Typesetting issues" displayName="Incorrect nesting"
                         enabledByDefault="true"
                         level="WEAK WARNING" />
        <localInspection language="Latex" implementationClass="nl.hannahsten.texifyidea.inspections.latex.typesetting.LatexCollapseCiteInspection"
                         groupPath="LaTeX" groupName="Typesetting issues" displayName="Collapse cite commands"
                         enabledByDefault="true"
                         level="WARNING" />
        <localInspection language="Latex" implementationClass="nl.hannahsten.texifyidea.inspections.latex.typesetting.LatexEnDashInspection"
                         groupPath="LaTeX" groupName="Typesetting issues" displayName="En dash in number ranges"
                         enabledByDefault="true"
                         level="WEAK WARNING" />
        <localInspection language="Latex" implementationClass="nl.hannahsten.texifyidea.inspections.latex.typesetting.LatexCdotInspection"
                         groupPath="LaTeX" groupName="Typesetting issues" displayName="Use of . instead of \cdot"
                         enabledByDefault="true"
                         level="WEAK WARNING" />
        <localInspection language="Latex" implementationClass="nl.hannahsten.texifyidea.inspections.latex.typesetting.LatexXInsteadOfTimesInspection"
                         groupPath="LaTeX" groupName="Typesetting issues" displayName="Use of x instead of \times"
                         enabledByDefault="true"
                         level="WEAK WARNING" />
        <localInspection language="Latex" implementationClass="nl.hannahsten.texifyidea.inspections.latex.typesetting.LatexVerticallyCenteredColonInspection"
                         groupPath="LaTeX" groupName="Typesetting issues" displayName="Vertically uncentered colon"
                         enabledByDefault="true"
                         level="WARNING" />
        <localInspection language="Latex" implementationClass="nl.hannahsten.texifyidea.inspections.latex.typesetting.LatexQedHereInspection"
                         groupPath="LaTeX" groupName="Typesetting issues" displayName="Insert \qedhere in trailing displaymath environment"
                         enabledByDefault="true"
                         level="WARNING" />
        <localInspection language="Latex" implementationClass="nl.hannahsten.texifyidea.inspections.latex.typesetting.LatexDiacriticIJInspection"
                         groupPath="LaTeX" groupName="Typesetting issues" displayName="Dotless versions of i and j must be used with diacritics"
                         enabledByDefault="true"
                         level="WARNING" />
        <localInspection language="Latex" implementationClass="nl.hannahsten.texifyidea.inspections.latex.typesetting.LatexEncloseWithLeftRightInspection"
                         groupPath="LaTeX" groupName="Typesetting issues" displayName="Enclose high commands with \leftX..\rightX"
                         enabledByDefault="true"
                         level="WEAK WARNING"/>
        <localInspection language="Latex" implementationClass="nl.hannahsten.texifyidea.inspections.latex.typesetting.LatexCiteBeforePeriodInspection"
                         groupPath="LaTeX" groupName="Typesetting issues" displayName="Citations must be placed before interpunction"
                         enabledByDefault="true"
                         level="WEAK WARNING" />
        <localInspection language="Latex" implementationClass="nl.hannahsten.texifyidea.inspections.latex.typesetting.LatexQuoteInspection"
                         groupPath="LaTeX" groupName="Typesetting issues" displayName="Incorrectly typeset quotation marks"
                         enabledByDefault="true"
                         level="WARNING" />
        <localInspection language="Latex" implementationClass="nl.hannahsten.texifyidea.inspections.latex.typesetting.LatexMissingGlossaryReferenceInspection"
                         groupPath="LaTeX" groupName="Code maturity" displayName="Missing glossary reference"
                         enabledByDefault="true"
                         level="WARNING" />
    </extensions>
</idea-plugin>